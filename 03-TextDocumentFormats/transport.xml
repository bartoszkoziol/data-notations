<?xml version="1.0" encoding="UTF-8"?>  <!-- Prolog XML: definiuje wersję i kodowanie dokumentu -->
<transportMEANS>                       <!-- Element główny: oznacza zbiór środków transportu -->
    <vehicle type="car" fuel="gasoline"> <!-- Element: pojazd, z atrybutami 'type' (typ pojazdu) i 'fuel' (paliwo) -->
        <brand>Toyota</brand>         <!-- Element: marka pojazdu -->
        <model>Corolla</model>       <!-- Element: model pojazdu -->
        <year>2023</year>            <!-- Element: rok produkcji pojazdu -->
        <color>White</color>         <!-- Element: kolor pojazdu -->
    </vehicle>                       <!-- Koniec elementu: pojazd -->
    
    <vehicle type="bicycle" gear="21">  <!-- Element: pojazd, z atrybutami 'type' (typ pojazdu) i 'gear' (liczba biegów) -->
        <brand>Trek</brand>           <!-- Element: marka roweru -->
        <model>FX 3</model>          <!-- Element: model roweru -->
        <year>2020</year>            <!-- Element: rok produkcji roweru -->
        <color>Black</color>         <!-- Element: kolor roweru -->
    </vehicle>                       <!-- Koniec elementu: pojazd -->
    
    <vehicle type="bus" capacity="50"> <!-- Element: pojazd, z atrybutami 'type' (typ pojazdu) i 'capacity' (pojemność) -->
        <route>45A</route>           <!-- Element: trasa autobusu -->
        <operator>City Transport</operator> <!-- Element: operator autobusu -->
        <year>2018</year>            <!-- Element: rok produkcji autobusu -->
        <color>Yellow</color>        <!-- Element: kolor autobusu -->
    </vehicle>                       <!-- Koniec elementu: pojazd -->

</transportMEANS>                     <!-- Koniec elementu głównego: zbiór środków transportu -->



